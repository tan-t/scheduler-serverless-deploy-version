{
  "functions": {
    "predeploy": "npm --prefix \"%RESOURCE_DIR%\" run build"
  },
  "hosting": {
    "predeploy": "npm --prefix \"./hosting\" run build",
    "public": "hosting/www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api",
        "function": "typeahead"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
